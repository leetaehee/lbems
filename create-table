	`stop_time` INT(11) NULL DEFAULT NULL,
	`error_code` CHAR(1) NULL DEFAULT NULL COLLATE 'latin1_swedish_ci',
	`error_code_com` CHAR(1) NULL DEFAULT NULL COLLATE 'latin1_swedish_ci',
	`error_in_temp` CHAR(1) NULL DEFAULT NULL COLLATE 'latin1_swedish_ci',
	`error_out_temp` CHAR(1) NULL DEFAULT NULL COLLATE 'latin1_swedish_ci',
	`error_bat` CHAR(1) NULL DEFAULT NULL COLLATE 'latin1_swedish_ci',
	`flow` INT(11) NULL DEFAULT NULL COMMENT '단위 : m3, 아마도 물',
	`flow_oil` FLOAT(12,2) NULL DEFAULT NULL COMMENT '단위 : 리터, 유량(기름)',
	`collect_type` VARCHAR(30) NULL DEFAULT NULL COMMENT '수집데이터 타입' COLLATE 'utf8_general_ci',
	`lora_hum` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`lora_temp` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`reg_date` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
	PRIMARY KEY (`sensor_sn`, `val_date`) USING BTREE,
	INDEX `collect_type` (`collect_type`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

