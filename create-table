	`limit_val_electric_equipment` VARCHAR(60) NULL DEFAULT NULL COMMENT '설비 목표값/기준값' COLLATE 'utf8_general_ci',
	`limit_val_co2` VARCHAR(60) NULL DEFAULT NULL COMMENT 'CO2 기준값' COLLATE 'utf8_general_ci',
	`electricType` VARCHAR(1) NULL DEFAULT 'H' COMMENT '계약종별' COLLATE 'utf8_general_ci',
	`typeGubun` VARCHAR(10) NULL DEFAULT NULL COMMENT '용도(low:저압,high:고압,type1:갑1,type2:갑2,type3:을)' COLLATE 'utf8_general_ci',
	`typeGubun2` VARCHAR(20) NULL DEFAULT NULL COMMENT '수전전압(low,high1,high2,high3)' COLLATE 'utf8_general_ci',
	`typeSelect` VARCHAR(20) NULL DEFAULT NULL COMMENT '요금제선택' COLLATE 'utf8_general_ci',
	`contractUseVal` INT(11) NULL DEFAULT NULL COMMENT '계약전력',
	`any_desk` VARCHAR(100) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	PRIMARY KEY (`complex_code_pk`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `bems_complex_device_info` (
	`complex_code_pk` VARCHAR(10) NOT NULL COLLATE 'utf8_general_ci',
	`type` VARCHAR(20) NOT NULL COMMENT 'solar, aircondition' COLLATE 'utf8_general_ci',
	`ip` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`port` INT(11) NULL DEFAULT NULL,
	`unit_id` INT(11) NULL DEFAULT NULL,
	`id` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`password` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`version` VARCHAR(50) NULL DEFAULT NULL COMMENT 'TEF : php, node.js 버전 구분' COLLATE 'utf8_general_ci',
	`maker` VARCHAR(20) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`memo` VARCHAR(50) NULL DEFAULT NULL COLLATE 'ucs2_general_ci',
	`reg_date` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
	`fg_del` CHAR(1) NOT NULL DEFAULT 'n' COLLATE 'utf8_general_ci',
	PRIMARY KEY (`complex_code_pk`, `type`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `bems_energy_buttons` (
	`button_id` INT(10) UNSIGNED NOT NULL AUTO_INCREMENT COMMENT 'PK',
	`group_id` INT(10) UNSIGNED NOT NULL COMMENT '메뉴 그룹 FK',
	`menu_id` INT(10) UNSIGNED NOT NULL COMMENT '메뉴 FK',
	`complex_code_pk` VARCHAR(10) NOT NULL COMMENT '단지코드' COLLATE 'utf8_general_ci',
	`reference_index` INT(10) UNSIGNED NOT NULL COMMENT '에너지원별 참조 인덱스',
	`button_order` INT(10) UNSIGNED NOT NULL COMMENT '버튼 화면 출력 순서\n',
	`name` VARCHAR(30) NOT NULL COMMENT '버튼 이름' COLLATE 'utf8_general_ci',
	`key` VARCHAR(30) NOT NULL COMMENT '버튼 키 (Jquery Selector 할 때 사용됨)\n' COLLATE 'utf8_general_ci',
	`is_use` CHAR(1) NULL DEFAULT 'N' COMMENT '메뉴에서 버튼 사용 여부' COLLATE 'utf8_general_ci',
	`memo` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`register` VARCHAR(20) NULL DEFAULT '케빈랩' COMMENT '등록자' COLLATE 'utf8_general_ci',
	`regi_date` TIMESTAMP NULL DEFAULT CURRENT_TIMESTAMP COMMENT '등록시각\n',
	`modifier` VARCHAR(20) NULL DEFAULT NULL COMMENT '수정자\n' COLLATE 'utf8_general_ci',
	`modi_date` DATETIME NULL DEFAULT NULL COMMENT '수정시각',
	PRIMARY KEY (`button_id`) USING BTREE,
	INDEX `idx_bems_energy_buttons_group_id` (`group_id`) USING BTREE,
	INDEX `idx_bems_energy_buttons_menu_id` (`menu_id`) USING BTREE,
	INDEX `idx_bems_energy_buttons_complex_code_pk` (`complex_code_pk`) USING BTREE,
	INDEX `idx_bems_energy_buttons_button_order` (`button_order`) USING BTREE
)
COMMENT='BEMS WEB 에너지원별 버튼'
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `bems_holiday` (
	`holiday_date` VARCHAR(15) NOT NULL COMMENT '휴일날짜' COLLATE 'utf8_general_ci',
	`is_holiday` CHAR(1) NULL DEFAULT 'N' COMMENT '공공기관 휴일여부' COLLATE 'utf8_general_ci',
	`holiday_name` VARCHAR(30) NULL DEFAULT NULL COMMENT '휴일명칭' COLLATE 'utf8_general_ci',
	`reg_date` TIMESTAMP NULL DEFAULT CURRENT_TIMESTAMP,
	PRIMARY KEY (`holiday_date`) USING BTREE
)
COMMENT='공휴일, 국경일 정보'
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `bems_home` (
	`complex_code_pk` VARCHAR(10) NOT NULL COLLATE 'utf8_general_ci',
	`home_dong_pk` VARCHAR(5) NOT NULL COLLATE 'utf8_general_ci',
	`home_ho_pk` VARCHAR(5) NOT NULL COMMENT '층별 전체전력은 190,191,192에서 시작할 것 , 사용하지 않거나 조회되지 않은 항목은 180,181,182에서 할것' COLLATE 'utf8_general_ci',
	`home_ho_nm` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`home_key_pk` VARCHAR(100) NOT NULL COMMENT '층별 전체 전력은 에너지원_건물코드_층 으로 구성' COLLATE 'utf8_general_ci',
	`home_grp_pk` VARCHAR(4) NULL DEFAULT NULL COMMENT '0M: 공동(메인전력,태양광 등 층에 상관없는 부분)\n1F: 1층\n2F: 2층\n3F: 3층\nPH:옥탑\nALL: 층 (1층전체전력, 2층전체전력..)' COLLATE 'utf8_general_ci',
	`home_type` VARCHAR(30) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`family_cnt` TINYINT(4) NULL DEFAULT NULL,
	`home_size` FLOAT(15,2) NULL DEFAULT NULL,
	`home_size_p` SMALLINT(11) NULL DEFAULT NULL,
	`fg_del` CHAR(1) NULL DEFAULT 'n' COLLATE 'latin1_swedish_ci',
	`reg_date` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
	`creator` INT(11) NULL DEFAULT NULL,
	`update_date` DATETIME NULL DEFAULT NULL,
	`updator` INT(11) NULL DEFAULT NULL,
	`limit_val_water` VARCHAR(60) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`limit_val_solar` VARCHAR(60) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`limit_val_gas` VARCHAR(60) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`limit_val_heating` VARCHAR(60) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`limit_val_hotwater` VARCHAR(60) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`limit_val_electric` VARCHAR(60) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`limit_val_electric_light` VARCHAR(60) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`limit_val_electric_cold` VARCHAR(60) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`limit_val_electric_elechot` VARCHAR(60) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`limit_val_electric_vent` VARCHAR(60) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`limit_val_electric_boiler` VARCHAR(60) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`limit_val_electric_cooking` VARCHAR(60) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`limit_val_electric_heating` VARCHAR(60) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`limit_val_electric_elevator` VARCHAR(60) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`limit_val_electric_hotwater` VARCHAR(60) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`limit_val_electric_water` VARCHAR(60) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`limit_val_electric_equipment` VARCHAR(60) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`limit_val_co2` VARCHAR(60) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	PRIMARY KEY (`complex_code_pk`, `home_dong_pk`, `home_ho_pk`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `bems_member` (
	`member_pk` INT(11) UNSIGNED NOT NULL AUTO_INCREMENT,
	`member_id` VARCHAR(50) NOT NULL COLLATE 'utf8_general_ci',
	`name` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`passwd` VARCHAR(255) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`hp` VARCHAR(15) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`join_date` DATETIME NULL DEFAULT NULL COMMENT '회원가입 시각\n',
	`fg_del` CHAR(1) NULL DEFAULT 'N' COLLATE 'utf8_general_ci',
	`fg_push` CHAR(1) NULL DEFAULT 'N' COLLATE 'utf8_general_ci',
	`last_login_date` DATETIME NULL DEFAULT NULL,
	`complex_code_pk` VARCHAR(10) NULL DEFAULT NULL COMMENT '단지' COLLATE 'utf8_general_ci',
	`home_dong_pk` VARCHAR(5) NULL DEFAULT NULL COMMENT '동' COLLATE 'utf8_general_ci',
	`home_ho_pk` VARCHAR(5) NULL DEFAULT NULL COMMENT '호' COLLATE 'utf8_general_ci',
	`modify_date` DATETIME NULL DEFAULT NULL COMMENT '회원정보 변경 시각\n',
	`withdrawal_date` DATETIME NULL DEFAULT NULL COMMENT '회원탈퇴 시각\n',
	PRIMARY KEY (`member_pk`) USING BTREE
)
COMMENT='세대원 테이블 '
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

