CREATE TABLE `ntek_meter_electric` (
	`sensor_sn` VARCHAR(20) NOT NULL COLLATE 'utf8_general_ci',
	`val_date` VARCHAR(15) NOT NULL COLLATE 'utf8_general_ci',
	`frequency` FLOAT(4,1) NULL DEFAULT NULL,
	`temperature` FLOAT(4,1) NULL DEFAULT NULL,
	`type` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`v_l1` FLOAT(10,1) NULL DEFAULT NULL,
	`v_l2` FLOAT(10,1) NULL DEFAULT NULL,
	`v_l3` FLOAT(10,1) NULL DEFAULT NULL,
	`v_ub` FLOAT(10,1) NULL DEFAULT NULL,
	`vpp_l1` FLOAT(10,1) NULL DEFAULT NULL,
	`vpp_l2` FLOAT(10,1) NULL DEFAULT NULL,
	`vpp_l3` FLOAT(10,1) NULL DEFAULT NULL,
	`i_l1` FLOAT(10,1) NULL DEFAULT NULL,
	`i_l2` FLOAT(10,1) NULL DEFAULT NULL,
	`i_l3` FLOAT(10,1) NULL DEFAULT NULL,
	`i_ub` FLOAT(10,1) NULL DEFAULT NULL,
	`watt` FLOAT(10,1) NULL DEFAULT NULL,
	`var` FLOAT(10,1) NULL DEFAULT NULL,
	`va` FLOAT(10,1) NULL DEFAULT NULL,
	`pf` FLOAT(10,1) NULL DEFAULT NULL,
	`kwh_imp` FLOAT(15,3) NULL DEFAULT NULL,
	`kwh_exp` FLOAT(15,3) NULL DEFAULT NULL,
	`kvarh_imp` FLOAT(10,1) NULL DEFAULT NULL,
	`kvarh_exp` FLOAT(10,1) NULL DEFAULT NULL,
	`error_code` TINYINT(4) NULL DEFAULT NULL,
	`reg_date` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
	PRIMARY KEY (`sensor_sn`, `val_date`) USING BTREE,
	INDEX `ntek_meter_electric_sensor_sn_index` (`sensor_sn`) USING BTREE,
	INDEX `ntek_meter_electric_val_date_index` (`val_date`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

