CREATE TABLE `reti_system_conf` (
	`serial` VARCHAR(50) NOT NULL COLLATE 'utf8_general_ci',
	`version` VARCHAR(20) NOT NULL COLLATE 'utf8_general_ci',
	`reg_date` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
	`model` VARCHAR(20) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`control_board_version` VARCHAR(20) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`module_version` VARCHAR(20) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`fg_del` CHAR(1) NULL DEFAULT 'n' COLLATE 'latin1_swedish_ci',
	PRIMARY KEY (`serial`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `toc_info` (
	`complex_code_pk` VARCHAR(10) NOT NULL DEFAULT '' COMMENT '건물코드' COLLATE 'utf8_general_ci',
	`type` VARCHAR(30) NOT NULL DEFAULT '' COMMENT '타입' COLLATE 'utf8_general_ci',
	`arch_type` VARCHAR(20) NULL DEFAULT NULL COMMENT '건축물타입' COLLATE 'utf8_general_ci',
	`dr_type` VARCHAR(20) NULL DEFAULT NULL COMMENT 'DR타입: 시민-국민-중소형 순서' COLLATE 'utf8_general_ci',
	`sensor_sn` VARCHAR(20) NULL DEFAULT NULL COMMENT '센서번호' COLLATE 'utf8_general_ci',
	PRIMARY KEY (`complex_code_pk`, `type`) USING BTREE,
	INDEX `sensor_sn` (`sensor_sn`) USING BTREE
)
COMMENT='TOC 정보 '
COLLATE='utf8_unicode_ci'
ENGINE=InnoDB
;