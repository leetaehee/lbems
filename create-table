CREATE TABLE `bems_admin` (
	`admin_pk` INT(10) UNSIGNED NOT NULL AUTO_INCREMENT,
	`admin_id` VARCHAR(50) NOT NULL COLLATE 'utf8_general_ci',
	`name` VARCHAR(255) NULL DEFAULT '-' COLLATE 'utf8_general_ci',
	`password` VARCHAR(255) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`complex_code_pk` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`home_dong_pk` VARCHAR(5) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`home_ho_pk` VARCHAR(5) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`login_level` INT(11) NULL DEFAULT '70',
	`email` VARCHAR(255) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`reg_date` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
	`creator` INT(11) NULL DEFAULT NULL,
	`update_date` DATETIME NULL DEFAULT NULL,
	`updator` INT(11) NULL DEFAULT NULL,
	`reg_status` CHAR(1) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`fg_del` CHAR(1) NULL DEFAULT 'n' COLLATE 'utf8_general_ci',
	`fg_connect` CHAR(1) NULL DEFAULT 'y' COLLATE 'utf8_general_ci',
	`last_login_date` DATETIME NULL DEFAULT NULL,
	`hp` VARCHAR(255) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`memo` VARCHAR(100) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`device_key` VARCHAR(40) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`auto_login` CHAR(1) NULL DEFAULT 'n' COLLATE 'utf8_general_ci',
	`rcv_mail` CHAR(1) NULL DEFAULT 'n' COLLATE 'utf8_general_ci',
	`rcv_push` CHAR(1) NULL DEFAULT 'n' COLLATE 'utf8_general_ci',
	`rcv_sms` CHAR(1) NULL DEFAULT 'n' COLLATE 'utf8_general_ci',
	`auth_num` VARCHAR(255) NULL DEFAULT NULL COMMENT '비밀번호 찾기할 때 인증번호' COLLATE 'utf8_general_ci',
	`login_fail_cnt` TINYINT(10) NULL DEFAULT '0' COMMENT '비밀번호 실패 횟수',
	`auth_valid_time` VARCHAR(15) NULL DEFAULT NULL COMMENT '비밀번호 찾기 시 인증번호 발급시간' COLLATE 'utf8_general_ci',
	`first_login_date` DATE NULL DEFAULT NULL COMMENT '최초 로그인 일자 (기본 비밀번호 변경을 요구하기 위함)',
	PRIMARY KEY (`admin_pk`, `admin_id`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `bems_admin_login_log` (
	`log_pk` INT(10) UNSIGNED NOT NULL AUTO_INCREMENT,
	`admin_id` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`ip_addr` VARCHAR(255) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`log_date` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
	`fg_login` CHAR(1) NULL DEFAULT NULL COMMENT '웹 접속여부' COLLATE 'utf8_general_ci',
	`fg_mobile` CHAR(1) NULL DEFAULT NULL COMMENT '앱 접속여부' COLLATE 'utf8_general_ci',
	`fg_api` CHAR(1) NULL DEFAULT NULL COMMENT 'API 로그인여부' COLLATE 'utf8_general_ci',
	`user_agent` VARCHAR(250) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	PRIMARY KEY (`log_pk`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `bems_aircondition_control_log` (
	`sensor_sn` VARCHAR(20) NOT NULL COLLATE 'utf8_general_ci',
	`val_date` VARCHAR(15) NOT NULL COLLATE 'utf8_general_ci',
	`complex_code_pk` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`exec_cmd` VARCHAR(100) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`updator` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`memo` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	PRIMARY KEY (`sensor_sn`, `val_date`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `bems_aircondition_status` (
	`sensor_sn` VARCHAR(20) NOT NULL COLLATE 'utf8_general_ci',
	`val_date` VARCHAR(15) NOT NULL COLLATE 'utf8_general_ci',
	`complex_code_pk` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`loc_floor` VARCHAR(4) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`location` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`power_on_off` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`set_temp` FLOAT(5,1) NULL DEFAULT NULL,
	`op_mode` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`air_swing_ud` VARCHAR(5) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`fan_speed` VARCHAR(5) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`reset_filter` VARCHAR(5) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`remote_con_enable` VARCHAR(5) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`room_temp` FLOAT(5,1) NULL DEFAULT NULL,
	PRIMARY KEY (`sensor_sn`, `val_date`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `bems_alarm_code` (
	`alarm_category` VARCHAR(20) NOT NULL DEFAULT '' COLLATE 'utf8_general_ci',
	`alarm_code` VARCHAR(10) NOT NULL DEFAULT '' COLLATE 'utf8_general_ci',
	`alarm_msg` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	PRIMARY KEY (`alarm_category`, `alarm_code`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `bems_alarm_log` (
	`no_alarm_log_pk` INT(11) NOT NULL AUTO_INCREMENT,
	`complex_code_pk` VARCHAR(10) NOT NULL DEFAULT '' COLLATE 'utf8_general_ci',
	`sensor_type` VARCHAR(30) NOT NULL DEFAULT '' COLLATE 'utf8_general_ci',
	`sensor_sn` VARCHAR(30) NOT NULL DEFAULT '' COLLATE 'utf8_general_ci',
	`alarm_code` VARCHAR(10) NOT NULL DEFAULT '' COLLATE 'utf8_general_ci',
	`alarm_on_off` VARCHAR(4) NOT NULL DEFAULT '' COLLATE 'utf8_general_ci',
	`alarm_msg` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`home_dong_pk` VARCHAR(5) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`home_ho_pk` VARCHAR(5) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`alarm_on_time` VARCHAR(15) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`alarm_off_time` VARCHAR(15) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`reg_date` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
	`fg_sms` CHAR(1) NULL DEFAULT '0' COLLATE 'latin1_swedish_ci',
	`fg_push` CHAR(1) NULL DEFAULT '0' COLLATE 'latin1_swedish_ci',
	`fg_mail` CHAR(1) NULL DEFAULT '0' COLLATE 'latin1_swedish_ci',
	PRIMARY KEY (`no_alarm_log_pk`, `complex_code_pk`, `sensor_type`, `sensor_sn`, `alarm_code`, `alarm_on_off`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `bems_api_account` (
	`target` VARCHAR(15) NOT NULL COMMENT '적용대상' COLLATE 'utf8_general_ci',
	`id` VARCHAR(30) NOT NULL COMMENT '아이디' COLLATE 'utf8_general_ci',
	`account_code` VARCHAR(255) NOT NULL COMMENT '계정코드' COLLATE 'utf8_general_ci',
	`password` VARCHAR(255) NOT NULL COMMENT '비밀번호' COLLATE 'utf8_general_ci',
	`access_token` VARCHAR(255) NULL DEFAULT NULL COMMENT 'JWT' COLLATE 'utf8_general_ci',
	`refresh_token` VARCHAR(255) NULL DEFAULT NULL COMMENT 'JWT' COLLATE 'utf8_general_ci',
	`client_key` VARCHAR(255) NULL DEFAULT NULL COMMENT '클라이언트 키' COLLATE 'utf8_general_ci',
	`iv_key` VARCHAR(255) NULL DEFAULT NULL COMMENT 'IV 키' COLLATE 'utf8_general_ci',
	`last_login_date` DATETIME NULL DEFAULT NULL COMMENT '최근 로그인 시각',
	`fg_use` CHAR(1) NULL DEFAULT 'n' COMMENT '계정 사용 여부 - 사용 \'y\', 사용안함 \'n\'' COLLATE 'utf8_general_ci',
	`register_date` TIMESTAMP NULL DEFAULT CURRENT_TIMESTAMP COMMENT '계정 생성 시각',
	`register` INT(11) NULL DEFAULT NULL COMMENT '등록자',
	`update_date` DATETIME NULL DEFAULT NULL COMMENT '정보 변경 시각',
	`updator` INT(11) NULL DEFAULT NULL COMMENT '변경자',
	PRIMARY KEY (`target`, `id`) USING BTREE,
	INDEX `id_hash` (`account_code`) USING BTREE
)
COMMENT='API 계정 정보'
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `bems_api_call_log` (
	`log_date` VARCHAR(15) NOT NULL COMMENT '로그시각' COLLATE 'utf8_general_ci',
	`ip` VARCHAR(30) NOT NULL COMMENT 'IP' COLLATE 'utf8_general_ci',
	`type` VARCHAR(30) NOT NULL COMMENT '에너지원' COLLATE 'utf8_general_ci',
	`url` VARCHAR(100) NULL DEFAULT NULL COMMENT 'API 호출 URL' COLLATE 'utf8_general_ci',
	`parameter` VARCHAR(6144) NULL DEFAULT NULL COMMENT '파라미터' COLLATE 'utf8_general_ci',
	`request_info` VARCHAR(6144) NULL DEFAULT NULL COMMENT 'API 호출 시 요청 정보' COLLATE 'utf8_general_ci',
	PRIMARY KEY (`log_date`, `ip`, `type`) USING BTREE
)
COMMENT='API 호출 로그 '
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `bems_api_integration_info` (
	`target` VARCHAR(15) NOT NULL COMMENT '적용대상' COLLATE 'utf8_general_ci',
	`complex_code_pk` VARCHAR(10) NOT NULL COMMENT '단지코드' COLLATE 'utf8_general_ci',
	`regi_date` TIMESTAMP NULL DEFAULT CURRENT_TIMESTAMP COMMENT '계정 생성 시각',
	`register` INT(11) NULL DEFAULT NULL COMMENT '등록자',
	`update_date` DATETIME NULL DEFAULT NULL COMMENT '정보 변경 시각',
	`updator` INT(11) NULL DEFAULT NULL COMMENT '변경자',
	PRIMARY KEY (`target`, `complex_code_pk`) USING BTREE
)
COMMENT='연계 API 단지 정보'
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `bems_autologin` (
	`admin_pk` INT(10) NOT NULL,
	`device_key` VARCHAR(50) NOT NULL COLLATE 'utf8_unicode_ci',
	`login_key` VARCHAR(50) NOT NULL COLLATE 'utf8_unicode_ci',
	`reg_date` TIMESTAMP NULL DEFAULT CURRENT_TIMESTAMP,
	`fg_del` CHAR(2) NULL DEFAULT 'n' COLLATE 'utf8_unicode_ci',
	`fg_mobile` CHAR(1) NULL DEFAULT 'n' COMMENT '모바일 앱 접속여부' COLLATE 'utf8_unicode_ci',
	PRIMARY KEY (`admin_pk`, `device_key`, `login_key`) USING BTREE
)
COLLATE='utf8_unicode_ci'
ENGINE=InnoDB
;

CREATE TABLE `bems_complex` (
	`complex_code_pk` VARCHAR(10) NOT NULL COLLATE 'utf8_general_ci',
	`name` VARCHAR(255) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`home_cnt` INT(11) NULL DEFAULT NULL COMMENT '세대 수',
	`home_dong_cnt` INT(11) NULL DEFAULT NULL COMMENT '동 수',
	`floor_cnt` INT(11) NULL DEFAULT NULL COMMENT '층 수',
	`bems_id` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`bld_id` VARCHAR(11) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`post_code` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`site_type` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`addr` VARCHAR(255) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`tel` VARCHAR(255) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`fax` VARCHAR(255) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`lat` DECIMAL(16,14) NULL DEFAULT NULL,
	`lon` DECIMAL(17,14) NULL DEFAULT NULL,
	`land_area` VARCHAR(15) NULL DEFAULT NULL COMMENT '로렌하우스에서 사용되는 컬럼, lbems에서는 사용되지 않는것으로 추정' COLLATE 'utf8_general_ci',
	`building_area` VARCHAR(15) NULL DEFAULT NULL COMMENT '건축면적' COLLATE 'utf8_general_ci',
	`comp_name` VARCHAR(20) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`service_start_date` DATE NULL DEFAULT NULL COMMENT '서비스 시작일',
	`closing_day_electric` CHAR(3) NULL DEFAULT NULL COLLATE 'latin1_swedish_ci',
	`closing_day_gas` CHAR(3) NULL DEFAULT NULL COLLATE 'latin1_swedish_ci',
	`closing_day_water` CHAR(3) NULL DEFAULT NULL COLLATE 'latin1_swedish_ci',
	`closing_day_heating` CHAR(3) NULL DEFAULT NULL COLLATE 'latin1_swedish_ci',
	`closing_day_hotwater` CHAR(3) NULL DEFAULT NULL COLLATE 'latin1_swedish_ci',
	`goal_electric` INT(11) NULL DEFAULT NULL,
	`goal_gas` INT(11) NULL DEFAULT NULL,
	`goal_water` INT(11) NULL DEFAULT NULL,
	`network_korea_api_key` CHAR(50) NULL DEFAULT NULL COLLATE 'latin1_swedish_ci',
	`memo` VARCHAR(20) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`fg_del` CHAR(1) NULL DEFAULT 'n' COLLATE 'latin1_swedish_ci',
	`fg_mobile` CHAR(1) NULL DEFAULT 'n' COMMENT '모바일 앱 사용유무' COLLATE 'utf8_general_ci',
	`manager` VARCHAR(255) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`hp` VARCHAR(15) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`reg_date` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
	`creator` INT(11) NULL DEFAULT NULL,
	`update_date` DATETIME NULL DEFAULT NULL,
	`updator` INT(11) NULL DEFAULT NULL,
	`air_station_name` VARCHAR(30) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`air_upd_date` DATETIME NULL DEFAULT NULL,
	`air_pm10` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`air_pm25` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`limit_val_water` VARCHAR(60) NULL DEFAULT NULL COMMENT '수도 목표값/기준값' COLLATE 'utf8_general_ci',
	`limit_val_solar` VARCHAR(60) NULL DEFAULT NULL COMMENT '태양광 목표값/기준값' COLLATE 'utf8_general_ci',
	`limit_val_gas` VARCHAR(60) NULL DEFAULT NULL COMMENT '가스 목표값/기준값' COLLATE 'utf8_general_ci',
	`limit_val_heating` VARCHAR(60) NULL DEFAULT NULL COMMENT '난방 목표값/기준값' COLLATE 'utf8_general_ci',
	`limit_val_hotwater` VARCHAR(60) NULL DEFAULT NULL COMMENT '급탕 목표값/기준값' COLLATE 'utf8_general_ci',
	`limit_val_electric` VARCHAR(60) NULL DEFAULT NULL COMMENT '전기 목표값/기준값' COLLATE 'utf8_general_ci',
	`limit_val_electric_light` VARCHAR(60) NULL DEFAULT NULL COMMENT '전등 목표값/기준값' COLLATE 'utf8_general_ci',
	`limit_val_electric_cold` VARCHAR(60) NULL DEFAULT NULL COMMENT '냉방 목표값/기준값' COLLATE 'utf8_general_ci',
	`limit_val_electric_elechot` VARCHAR(60) NULL DEFAULT NULL COMMENT '전열 목표값/기준값' COLLATE 'utf8_general_ci',
	`limit_val_electric_vent` VARCHAR(60) NULL DEFAULT NULL COMMENT '환기 목표값/기준값' COLLATE 'utf8_general_ci',
	`limit_val_electric_boiler` VARCHAR(60) NULL DEFAULT NULL COMMENT '보일러 목표값/기준값' COLLATE 'utf8_general_ci',
	`limit_val_finedust` VARCHAR(60) NULL DEFAULT NULL COMMENT '미세먼지, 초미세먼지, 극초미세먼지 목표값/기준값' COLLATE 'utf8_general_ci',
	`limit_val_electric_cooking` VARCHAR(60) NULL DEFAULT NULL COMMENT '취사 목표값/기준값' COLLATE 'utf8_general_ci',
	`limit_val_electric_heating` VARCHAR(60) NULL DEFAULT NULL COMMENT '전기 난방 목표값/기준값' COLLATE 'utf8_general_ci',
	`limit_val_electric_elevator` VARCHAR(60) NULL DEFAULT NULL COMMENT '승강 목표값/기준값' COLLATE 'utf8_general_ci',
	`limit_val_electric_hotwater` VARCHAR(60) NULL DEFAULT NULL COMMENT '전기급탕 목표값/기준값' COLLATE 'utf8_general_ci',
