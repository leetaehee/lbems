	`sensor_sn` VARCHAR(20) NOT NULL COLLATE 'utf8_general_ci',
	`type` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`val_date` VARCHAR(15) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`kwh_imp` FLOAT(15,3) NULL DEFAULT NULL,
	`kwh_exp` FLOAT(15,3) NULL DEFAULT NULL,
	`watt` FLOAT(10,1) NULL DEFAULT NULL,
	`pf` FLOAT(10,1) NULL DEFAULT NULL,
	`error_code` INT(11) NULL DEFAULT NULL,
	`place` VARCHAR(20) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`reg_date` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
	`fg_use` CHAR(1) NULL DEFAULT 'y' COLLATE 'latin1_swedish_ci',
	`memo` VARCHAR(20) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`maker` VARCHAR(10) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`all_data` VARCHAR(6144) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	PRIMARY KEY (`sensor_sn`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `rems_alarm_code` (
	`alarm_code_pk` INT(10) UNSIGNED NOT NULL AUTO_INCREMENT,
	`plante_pk` INT(11) NULL DEFAULT NULL,
	`alarm_category` VARCHAR(20) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`alarm_code` INT(11) NULL DEFAULT NULL,
	`alarm_msg` VARCHAR(100) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	PRIMARY KEY (`alarm_code_pk`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `rems_alarm_log` (
	`alarm_log_pk` INT(10) UNSIGNED NOT NULL AUTO_INCREMENT,
	`alarm_category` VARCHAR(20) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`alarm_value` VARCHAR(3) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`alarm_msg` VARCHAR(100) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`plante_pk` INT(11) NULL DEFAULT NULL,
	`rtu_sn` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`inverter_name` VARCHAR(30) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`type` VARCHAR(30) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`alarm_time` DATETIME NULL DEFAULT NULL,
	`alarm_off_time` DATETIME NULL DEFAULT NULL,
	`alarm_on_time` DATETIME NULL DEFAULT NULL,
	`alarm_on_off` VARCHAR(4) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`reg_date` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
	`etc` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`fg_sms` CHAR(1) NULL DEFAULT '0' COLLATE 'latin1_swedish_ci',
	`fg_push` CHAR(1) NULL DEFAULT '0' COLLATE 'latin1_swedish_ci',
	`fg_mail` CHAR(1) NULL DEFAULT '0' COLLATE 'latin1_swedish_ci',
	PRIMARY KEY (`alarm_log_pk`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `reti_component_info` (
	`serial` VARCHAR(50) NOT NULL COLLATE 'utf8_general_ci',
	`type` VARCHAR(20) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`reg_date` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
	`version` VARCHAR(20) NOT NULL COLLATE 'utf8_general_ci',
	`inverter_name` VARCHAR(30) NOT NULL COLLATE 'utf8_general_ci',
	`group_range` VARCHAR(20) NOT NULL COLLATE 'utf8_general_ci',
	`fg_del` CHAR(1) NULL DEFAULT 'n' COLLATE 'latin1_swedish_ci',
	`vendor` VARCHAR(30) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	`model` VARCHAR(20) NULL DEFAULT NULL COLLATE 'utf8_general_ci',
	PRIMARY KEY (`serial`, `inverter_name`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `reti_groups` (
	`serial` VARCHAR(50) NOT NULL COLLATE 'utf8_general_ci',
	`group_id` INT(11) NOT NULL,
	`inverter_name` VARCHAR(30) NOT NULL COLLATE 'utf8_general_ci',
	`reg_date` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
	`name` VARCHAR(20) NOT NULL COLLATE 'utf8_general_ci',
	`type` VARCHAR(50) NOT NULL COLLATE 'utf8_general_ci',
	`elec_type` VARCHAR(50) NOT NULL COLLATE 'utf8_general_ci',
	`fg_del` CHAR(1) NULL DEFAULT 'n' COLLATE 'latin1_swedish_ci',
	PRIMARY KEY (`serial`, `group_id`, `inverter_name`, `reg_date`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `reti_group_data_1_raw_data` (
	`group_id` INT(11) NOT NULL,
	`serial` VARCHAR(50) NOT NULL COLLATE 'utf8_general_ci',
	`val_date` VARCHAR(50) NOT NULL COLLATE 'utf8_general_ci',
	`reg_date` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
	`average_temperature` FLOAT(15,2) NULL DEFAULT NULL,
	`average_dc_voltage` FLOAT(15,2) NULL DEFAULT NULL,
	`sum_dc_current` FLOAT(15,2) NULL DEFAULT NULL,
	`sum_dc_electric_power` FLOAT(15,2) NULL DEFAULT NULL,
	`sum_dc_ampere` FLOAT(15,2) NULL DEFAULT NULL,
	`accumulate_dc_power_consumption` FLOAT(15,2) NULL DEFAULT NULL,
	`pv_temperature` FLOAT(15,2) NULL DEFAULT NULL,
	`solar_radiation_quantity` FLOAT(15,2) NULL DEFAULT NULL,
	PRIMARY KEY (`group_id`, `serial`, `val_date`) USING BTREE
)
COMMENT='DC data'
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `reti_group_data_2_raw_data` (
	`group_id` INT(11) NOT NULL,
	`serial` VARCHAR(50) NOT NULL COLLATE 'utf8_general_ci',
	`val_date` VARCHAR(50) NOT NULL COLLATE 'utf8_general_ci',
	`reg_date` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
	`average_phase_voltage` FLOAT(15,2) NULL DEFAULT NULL,
	`average_line_voltage` FLOAT(15,2) NULL DEFAULT NULL,
	`average_phase_current` FLOAT(15,2) NULL DEFAULT NULL,
	`sum_active_power` FLOAT(15,2) NULL DEFAULT NULL,
	`sum_reactive_power` FLOAT(15,2) NULL DEFAULT NULL,
	`power_factor` FLOAT(15,2) NULL DEFAULT NULL,
	`average_frequency` FLOAT(15,2) NULL DEFAULT NULL,
	`average_thd_phase_voltage` FLOAT(15,2) NULL DEFAULT NULL,
	`average_thd_phase_current` FLOAT(15,2) NULL DEFAULT NULL,
	`average_temperature` FLOAT(15,2) NULL DEFAULT NULL,
	`sum_apparent_power` FLOAT(15,2) NULL DEFAULT NULL,
	`accumulate_power_consumption` FLOAT(15,2) NULL DEFAULT NULL,
	`raw_accumulate_power_consumption` FLOAT(15,2) NULL DEFAULT NULL,
	`electric_charge` FLOAT(15,2) NULL DEFAULT NULL,
	`line_voltage_t_r` FLOAT(15,2) NULL DEFAULT NULL,
	`line_voltage_s_t` FLOAT(15,2) NULL DEFAULT NULL,
	`line_voltage_r_s` FLOAT(15,2) NULL DEFAULT NULL,
	`inverter_conversion_efficiency` FLOAT(15,2) NULL DEFAULT NULL,
	PRIMARY KEY (`group_id`, `serial`, `val_date`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `reti_modules` (
	`serial` VARCHAR(50) NOT NULL COLLATE 'utf8_general_ci',
	`group_id` INT(11) NULL DEFAULT NULL,
	`inverter_name` VARCHAR(30) NOT NULL COLLATE 'utf8_general_ci',
	`module_id` INT(11) NOT NULL,
	`reg_date` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
	`name` VARCHAR(20) NOT NULL COLLATE 'utf8_general_ci',
	`phase` VARCHAR(20) NOT NULL COLLATE 'utf8_general_ci',
	`rate` VARCHAR(10) NOT NULL COLLATE 'utf8_general_ci',
	`elec_type` VARCHAR(10) NOT NULL COLLATE 'utf8_general_ci',
	`fg_del` CHAR(1) NULL DEFAULT 'n' COLLATE 'latin1_swedish_ci',
	PRIMARY KEY (`serial`, `module_id`, `inverter_name`, `reg_date`) USING BTREE
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

