CREATE TABLE `bems_menus` (
	`menu_id` INT(10) UNSIGNED NOT NULL AUTO_INCREMENT COMMENT 'PK',
	`group_id` INT(10) UNSIGNED NOT NULL COMMENT '메뉴 그룹 FK\n',
	`complex_code_pk` VARCHAR(10) NOT NULL COMMENT '단지코드\n' COLLATE 'utf8_general_ci',
	`menu_order` INT(10) UNSIGNED NOT NULL COMMENT '순서\\n',
	`name` VARCHAR(80) NOT NULL COLLATE 'utf8_general_ci',
	`is_use` CHAR(1) NULL DEFAULT 'N' COMMENT '메뉴 사용여부\n' COLLATE 'utf8_general_ci',
	`url` VARCHAR(255) NOT NULL COMMENT 'URL\\\\n' COLLATE 'utf8_general_ci',
	`register` VARCHAR(20) NULL DEFAULT '케빈랩' COMMENT '등록자\\\\\\\\\\\\\\\\n' COLLATE 'utf8_general_ci',
	`regi_date` TIMESTAMP NULL DEFAULT CURRENT_TIMESTAMP COMMENT '등록시각\n',
	`modifier` VARCHAR(20) NULL DEFAULT NULL COMMENT '수정자\\\\n' COLLATE 'utf8_general_ci',
	`modi_date` DATETIME NULL DEFAULT NULL COMMENT '수정시각\\\\n',
	PRIMARY KEY (`menu_id`) USING BTREE,
	INDEX `idx_bems_menus_group_id` (`group_id`) USING BTREE,
	INDEX `idx_bems_menus_complex_code_pk` (`complex_code_pk`) USING BTREE,
	INDEX `idx_bems_menus_menu_order` (`menu_order`) USING BTREE,
	INDEX `idx_bems_menus_name` (`name`) USING BTREE,
	INDEX `url` (`url`) USING BTREE
)
COMMENT='BEMS WEB 서브 메뉴'
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `bems_menu_groups` (
	`group_id` INT(11) UNSIGNED NOT NULL AUTO_INCREMENT COMMENT 'PK\\n',
	`complex_code_pk` VARCHAR(45) NOT NULL COMMENT '단지코드' COLLATE 'utf8_general_ci',
	`menu_order` INT(11) UNSIGNED NOT NULL COMMENT '순서',
	`name` VARCHAR(80) NOT NULL COMMENT '메뉴그룹 이름' COLLATE 'utf8_general_ci',
	`is_use` CHAR(1) NULL DEFAULT 'N' COMMENT '메뉴그룹 사용여부\n' COLLATE 'utf8_general_ci',
	`is_use_sub_menu` CHAR(1) NULL DEFAULT 'N' COMMENT '서브메뉴 사용여부\n' COLLATE 'utf8_general_ci',
	`url` VARCHAR(255) NULL DEFAULT NULL COMMENT 'URL' COLLATE 'utf8_general_ci',
	`icon` VARCHAR(100) NOT NULL COMMENT '아이콘\\\\n' COLLATE 'utf8_general_ci',
	`authority` INT(11) UNSIGNED NULL DEFAULT '70' COMMENT '권한\\n',
	`is_default_menu` CHAR(1) NULL DEFAULT 'N' COMMENT '디폴트메뉴 사용여부\\n' COLLATE 'utf8_general_ci',
	`is_use_dashboard` CHAR(1) NULL DEFAULT 'N' COMMENT '대시보드 메뉴 사용여부 (하위메뉴가 없는 그룹메뉴에만 적용 할 수 있도록 개발 할 것)\\n' COLLATE 'utf8_general_ci',
	`register` VARCHAR(20) NULL DEFAULT '케빈랩' COMMENT '등록자\\\\\\\\n' COLLATE 'utf8_general_ci',
	`regi_date` TIMESTAMP NULL DEFAULT CURRENT_TIMESTAMP COMMENT '등록시각\\\\n',
	`modifier` VARCHAR(20) NULL DEFAULT NULL COMMENT '수정자\n' COLLATE 'utf8_general_ci',
	`modi_date` DATETIME NULL DEFAULT NULL COMMENT '수정시각',
	PRIMARY KEY (`group_id`) USING BTREE,
	INDEX `idx_bems_menu_groups_complex_code_pk` (`complex_code_pk`) USING BTREE,
	INDEX `idx_bems_menu_groups_name` (`name`) USING BTREE,
	INDEX `idx_bems_menu_groups_menu_order` (`menu_order`) USING BTREE,
	INDEX `url` (`url`) USING BTREE
)
COMMENT='BEMS WEB 메뉴 그룹'
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

